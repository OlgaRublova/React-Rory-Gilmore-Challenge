     const rect = bookRef.current.getBoundingClientRect();\n        return rect.top >= 0 && rect.bottom <= window.innerHeight;\n    };\n\n    const scrollHandler = () => {\n        setInView(isInView());\n    }\n\n    useEffect(() => {\n        setIsLoading(false)\n\n        setInView(isInView());\n        window.addEventListener(\"scroll\", scrollHandler);\n        return () => {\n            window.removeEventListener(\"scroll\", scrollHandler);\n        }\n\n    }, [])\n\n\n    return(\n        <img\n            src={cover}\n            alt={title}\n            ref={bookRef}\n            className={isLoading ? null : inView ? \"book-img\" : \"book-img-shadow\"}\n        />\n    )\n}\n\nexport default ImageColorOnScroll;","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/LengthButtons.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/BooksView.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListColumns.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListItem.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListTotals.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Pages.js",["205","206","207"],"import React from \"react\";\nimport {useFilterContext} from \"../context/filter_context\"\n\nconst Pages = () => {\n    const {\n        paginated_books,\n        handlePage,\n        page,\n\n    } = useFilterContext();\n\n\n    return (\n\n          <section className=\"btn-container--page\">\n                {/*{*/}\n                {/*    paginated_books.map((item, index) => {*/}\n                {/*        return (*/}\n                {/*            <button*/}\n\n                {/*                key={index}*/}\n                {/*                className={`pagination-btn ${index === page ? 'pagination-btn--active' : null}`}*/}\n             