e, cover, firstName, lastName, genre, page} = singleBook;\n\n    return (\n\n        <section className=\"single-book\">\n            <div className=\"single-book-arrows\">\n                <button className=\"single-book-arrow\">\n                    <AiOutlineArrowLeft/>\n                </button>\n                <button className=\"single-book-arrow\">\n                    <AiOutlineArrowRight/>\n                </button>\n            </div>\n\n            <Link to=\"/books\" className=\"btn-primary\">Back to books</Link>\n\n            <h2 className=\"section-heading\">{title}</h2>\n            <img src={cover} alt={title}/>\n            <div className=\"single-book-info\">\n\n                <p><span className=\"book-data\">Author:</span> {firstName} {lastName}</p>\n                <p><span className=\"book-data\">Genre:</span> {genre}</p>\n                <p><span className=\"book-data\">Count:</span> {page} pages</p>\n            </div>\n        </section>\n\n\n    )\n}\n\nexport default SingleBookPage;","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Navbar.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/reducer/filter_reducer.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/reducer/list_reducer.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/reducer/books_reducer.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Title.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Auth.js",["193"],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/index.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListContent.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/data.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/utils.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/helpers/links.js",["194"],"import React from 'react'\nimport {BiHomeHeart, BsBook, AiOutlineCoffee, AiOutlineComment, BiHeart} from 'react-icons/all'\n\nconst navbarContainerLinks = [\n    {\n        title: \"Home\",\n        icon: <BiHomeHeart/>,\n        url: \"/\",\n        id: 4,\n    },\n    {\n        title: \"About Olga\",\n        icon: <AiOutlineComment/>,\n        url: \"/olga\",\n        id: 1,\n    },\n    {\n        title: \"About Rory\",\n        icon: <AiOutlineCoffee/>,\n        url: \"/rory\",\n        id: 2,\n    },\n    {\n        title: \"Books\",\n        icon: <BsBook/>,\n        url: \"/books\",\n        id: 3,\n    },\n    {\n        title: \"Wish List\",\n        icon: <BiHeart/>,\n        url: \"/wish-list\",\n        id: 5,\n    },\n    // {\n    //     title: \"Have read\",\n    //     icon: <RiBook2Line/>,\n    //     url: \"/read-list\",\n    //     id: 6,\n    // },\n    // {\n    //     title: \"Have liked\",\n    //     icon: <RiBook2Line/>,\n    //     url: \"/liked-list\",\n    //     id: 7,\n    // },\n\n]\n\nconst length = [\n    {\n        id: 1,\n        size: \"XS\"\n    },\n    {\n        id: 2,\n        size: \"S\"\n    },\n    {\n        id: 3,\n        size: \"M\"\n    },\n    {\n        id: 4,\n        size: \"L\"\n    },\n    {\n        id: 5,\n        size: \"XL\"\n    },\n]\n\n//export default {length, links};\nexport default  navbarContainerLinks;","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Genres.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ImageColorOnScroll.js",["195"],"import React, {useEffect, useRef, useState} from \"react\";\n\n\nconst ImageColorOnScroll = ({title, cover}) => {\n    const bookRef = useRef(null);\n\n    const [inView, setInView] = useState(false);\n    const [isLoading, setIsLoading] = useState(false)\n\n    const isInView = () => {\n        const rect = bookRef.current.getBoundingClientRect();\n        return rect.top >= 0 && rect.bottom <= window.innerHeight;\n    };\n\n    const scrollHandler = () => {\n        setInView(isInView());\n    }\n\n    useEffect(() => {\n        setIsLoading(false)\n\n        setInView(isInView());\n        window.addEventListener(\"scroll\", scrollHandler);\n        return () => {\n            window.removeEventListener(\"scroll\", scrollHandler);\n        }\n\n    }, [])\n\n\n    return(\n        <img\n            src={cover}\n            alt={title}\n            ref={bookRef}\n            className={isLoading ? null : inView ? \"book-img\" : \"book-img-shadow\"}\n        />\n    )\n}\n\nexport default ImageColorOnScroll;","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/LengthButtons.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/BooksView.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListColumns.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListItem.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListTotals.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Pages.js",["196","197","198"],"import React from \"react\";\nimport {useFilterContext} from \"../context/filter_context\"\n\nconst Pages = () => {\n    const {\n        paginated_books,\n        handlePage,\n        page,\n\n    } = useFilterContext();\n\n\n    return (\n\n          <section className=\"btn-container--page\">\n                {/*{*/}\n                {/*    paginated_books.map((item, index) => {*/}\n                {/*        return (*/}\n                {/*            <button*/}\n\n                {/*                key={index}*/}\n                {/*                className={`pagination-btn ${index === page ? 'pagination-btn--active' : null}`}*/}\n                {/*           