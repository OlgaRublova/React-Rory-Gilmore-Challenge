er = () => {\n            try {\n                axios.put(\"/reviews/\" + review._id + \"/like\", {userId: user._id})\n            } catch (err) {\n                console.log(err)\n            }\n            setLike(isLiked ? like - 1 : like + 1);\n            setIsLiked(!isLiked)\n        }\n\n\n        return (\n            <div className=\"reviews-container__review\">\n                <div className=\"review__stars\">\n                    <ReactStars\n                        value={ratingsQuantity}\n                        count={5}\n                        size={24}\n                        activeColor=\"#ffd700\"\n                        isHalf={false}\n                        edit={false}\n                    />\n                </div>\n                <div className=\"review__info\">\n\n                    <h1 className=\"review__info__title\">{headline}</h1>\n                    <div className=\"review__info__detail\">{detail}</div>\n                    <div className=\"review__info__likes\">\n                        <div className=\"review__info__likes__heartBox\">\n                            <BsFillHeartFill\n                                onClick={likeHandler}\n                                className={isLiked ? \"favorite\" : null}\n                            />\n                            <span>{like} like(s)</span>\n                        </div>\n                        {\n                            user._id == userId &&\n                            <>\n                                <Link to={`/edit/${_id}`}>\n                                    <BiEdit className=\"link\"/>\n                                </Link>\n                                <AiFillDelete onClick={() => removeReview(_id)}/>\n                            </>\n                        }\n\n                    </div>\n\n                </div>\n                <div className=\"user__info\">\n                    <div className=\"user__info__fullName\">{username}</div>\n       