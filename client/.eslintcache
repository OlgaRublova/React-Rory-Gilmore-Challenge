n            })\n            return {\n                ...state,\n                checked_items: tempList\n            }\n        case \"CLEAR_LIST\":\n            return {\n                ...state,\n                checked_items: []\n            }\n\n        case \"LIKE_BOOK\":\n            return {\n                ...state,\n                liked_items: action.payload\n            }\n\n        case \"CHECK_BOOK\":\n            return {\n                ...state,\n                checked_items: action.payload\n            }\n\n        default:\n            return state\n    }\n}\n\n\nexport default list_reducer","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Title.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Auth.js",["180"],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/index.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListContent.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/helpers/links.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Genres.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ImageColorOnScroll.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/LengthButtons.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListColumns.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListItem.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/ListTotals.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Pulitzer.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/components/Sort.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/helpers/image-urls.js",[],"/Users/olga/W