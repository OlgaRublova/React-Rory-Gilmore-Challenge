           <input\n                                    name=\"password\"\n                                    type=\"password\"\n                                    placeholder=\"Password\"\n                                    onChange={handleChange}\n                                    required\n                                />\n                            </div>\n\n\n                            <div className=\"auth__form-container_fields-content_button\">\n                                <button onClick={handleSubmit}>Sign In</button>\n                            </div>\n                        </form>\n\n                        <div className=\"auth__form-container_fields-account\">\n                            <p> Don't have an account?\n                                <Link to=\"/register\">\n                                    <span> Sign Up</span>\n                                </Link>\n                            </p>\n                        </div>\n                    </div>\n                </div>\n                <div className=\"auth__form-container_image\">\n                    <img src={signInImage} alt=\"sign up\"/>\n                </div>\n            </div>\n        </section>\n    )\n}\n\n\nexport default Login;","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/context/user_context.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/client/src/reducer/user_reducer.js",[],{"ruleId":"203","severity":1,"message":"204","line":2,"column":9,"nodeType":"205","messageId":"206","endLine":2,"endColumn":13},{"ruleId":"203","severity":1,"message":"207","line":3,"column":8,"nodeType":"205","messageId":"206","endLine":3,"endColumn":12},{"ruleId":"208","severity":1,"message":"209","line":24,"column":8,"nodeType":"210","endLine":24,"endColumn":19,"suggestions":"211"},{"ruleId":"203","severity":1,"message":"212","line":4,"column":8,"nodeType":"205","messageId":"206","endLine":4,"endColumn":13},{"ruleId":"203","severity":1,"m