rmProjects/React-Rory-Gilmore-Challenge/rory/src/components/books.js",[],["105","106"],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/rory/src/contex.js",["107","108"],"import React, {useState, useContext, useEffect} from \"react\";\nimport booksList from \"./components/data\";\nimport {useFetch} from \"./components/useFetch\";\nimport paginate from \"./components/utils\";\n\nconst AppContext = React.createContext();\n\nconst allGenres = ['all', ...new Set(booksList.map((book) => book.genre))];\n\nconsole.clear();\n\nconst AppProvider = ({children}) => {\n    const {\n        showPagination,\n        setShowPagination,\n        paginatedBooks,\n        books,\n        setBooks,\n        page,\n        setPage,\n    } = useFetch();\n\n    const [genres, setGenres] = useState(allGenres);\n\n    const [isModalOpen, setIsModalOpen] = useState(false);\n    const [isModalNoOpen, setIsNoModalOpen] = useState(false);\n    const [error, setError] = useState({show: false, msg: \"\"})\n\n    useEffect(() => {\n        if (showPagination) return;\n        setBooks(paginatedBooks[page]);\n    }, [showPagination, page])\n\n    const openModal = () => {\n        setIsModalOpen(true);\n    }\n    const closeModal = () => {\n        setIsModalOpen(false)\n\n    }\n    const openNoModal = () => {\n        setIsNoModalOpen(true);\n    }\n    const closeNoModal = () => {\n        setIsNoModalOpen(false);\n\n    }\n\n\n    const handlePage = index => {\n        setPage(index);\n    }\n    const prevPage = () => {\n        setPage(oldPage => {\n            let prevPage = oldPage - 1;\n            if (prevPage < 0) {\n                prevPage = paginatedBooks.length - 1;\n            }\n            return prevPage;\n        })\n    }\n    const nextPage = () => {\n        setPage(oldPage => {\n            let nextPage = oldPage + 1;\n            if (nextPage > paginatedBooks.length - 1) {\n                nextPage = 0;\n            }\n            return nextPage;\n        })\n    }\n\n    const filterBooksByGenre = (genre) => {\n\n        if (genre === 'all') {\n            setBooks(booksList);\n            setShowPagination(false);\n            return;\n        } else {\n            const newBooks = booksList.filter((book) => book.genre === genre);\n\n            if (newBooks.length > 15) {\n                setBooks(paginate(newBooks));\n                setShowPagination(false)\n            } else {\n                setBooks(newBooks);\n                setShowPagination(true);\n            }\n        }\n    }\n\n    const removeBook = id => {\n        return setBooks(books.filter((book) => book.id !== id));\n    }\n\n    function toggleError(show = false, msg = \"\") {\n        setError({show, msg})\n    }\n\n    return (\n        <AppContext.Provider value={{\n            genres,\n            page, setPage, prevPage, nextPage, handlePage,\n            isModalOpen, setIsModalOpen, openModal, closeModal,\n            isModalNoOpen, setIsNoModalOpen, openNoModal, closeNoModal,\n            error, toggleError,\n            books, setBooks, removeBook, filterBooksByGenre,\n            paginatedBooks, showPagination, setShowPagination,\n\n        }}>\n            {children}\n        </AppContext.Provider>\n    )\n}\n\nexport const useGlobalContext = () => {\n    return useContext(AppContext)\n}\n\nexport {AppContext, AppProvider}","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/rory/src/components/SearchForm.js",["109","110"],"import {useGlobalContext} from \"../contex\";\nimport {MdSearch} from \"react-icons/all\";\nimport {useRef, useEffect, useState} from \"react\";\nimport bookList from \"./data.js\"\n\nconst SearchForm = () => {\n    const {error, toggleError, books, setBooks} = useGlobalContext();\n\n    const [searchTerm, setSearchTerm] = useState('');\n\n    const handleSubmit = e => {\n        e.preventDefault();\n        toggleError(true, \"sorry, there is no book like that\")\n    }\n\n    const searchValue = useRef('');\n\n    useEffect(() => {\n        searchValue.current.focus()\n    }, []);\n\n    useEffect(() => {\n        findBooks(searchTerm)\n\n    }, [searchTerm]);\n\n    const findBooks = (searchItem) => {\n        let specificBooks = bookList.filter(book => {\n            const {title, firstName, lastName} = book;\n\n            return (\n                title.toString().toLowerCase().includes(searchItem.toString().toLowerCase()) ||\n                firstName.toString().toLowerCase().includes(searchItem.toString().toLowerCase()) ||\n                lastName.toString().toLowerCase().includes(searchItem.toString().toLowerCase())\n            )\n        });\n\n        setBooks(specificBooks);\n    }\n\n\n    return (\n        <>\n\n            <article className=\"section-text\">\n                <div className=\"section-text__header\">Great!</div>\n                <div className=\"section-text__sub\">Check if your favourite book is in the list</div>\n            </article>\n            <div className=\"errorMessage\">{error.show && <p>{error.msg}</p>}</div>\n\n\n            <form\n                className=\"section-heading\"\n                onSubmit={handleSubmit}>\n                <div className=\"form-control\">\n                    <MdSearch/>\n                    <input\n                        type=\"text\"\n                        ref={searchValue}\n                        onChange={(e) => setSearchTerm(searchValue.current.value)}\n                    />\n                    <button>search</button>\n                </div>\n\n            </form>\n        </>\n    )\n}\n\n\nexport default SearchForm;","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/rory/src/components/Books.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/rory/src/components/Pages.js",["111"],"import {useGlobalContext} from \"../contex\";\nimport {useEffect} from \"react\";\n\nconst Pages = () => {\n    const {\n        paginatedBooks,\n        prevPage,\n        nextPage,\n        handlePage,\n        page,\n        isLoading,setBooks, genres\n    } = useGlobalContext();\n\n\n\n\n    return (\n        <section className=\"btn-container--page\">\n            <button className=\"prev-btn\" onClick={prevPage}>prev</button>\n            {\n                paginatedBooks.map((item, index) => {\n                    return (\n                        <button\n                            key={index}\n                            className={`page-btn ${index === page ? 'active-btn' : null}`}\n                            onClick={() => {\n                                handlePage(index)\n                            }}>\n                            {index + 1}\n                        </button>\n                    )\n                })\n            }\n            <button className=\"next-btn\" onClick={nextPage}>next</button>\n        </section>\n    )\n}\n\nexport default Pages;","/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/rory/src/components/Header.js",[],"/Users/olga/WebstormProjects/React-Rory-Gilmore-Challenge/rory/src/components/SingleBook.js",["112","113"],"import {useParams, Link} from \"react-router-dom\"\nimport {AiOutlineArrowLeft, AiOutlineArrowRight} from \"react-icons/all\";\nimport data from \"./data\"\nimport {BiBook} from \"react-icons/all\";\n\nconst SingleBook = () => {\n\n    let {id} = useParams(),\n        result,\n        index = 0;\n\n    data.forEach(function check(el, index, array) {\n        if (el.id == id) {\n            result = index;\n        } else {\n            index++;\n        }\n    })\n\n    const {title, cover, firstName, lastName, genre, page} = data[result];\n\n    const prevBook = () => {\n    }\n    const nextBook = () => {\n    }\n\n    return (\n        <>\n\n\n            <section className=\"single-book\">\n                <div className=\"arrows\">\n                    <button className=\"arrow-btn\" onClick={prevBook}><AiOutlineArrowLeft/></button>\n                    <button c